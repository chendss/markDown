  "scripts": {
    "build-dll": "webpack -p --display-error-details --progress --colors --config webpack.dll.config.js",
    "start": "yarn install && npm run build-dll && cross-env ENV=loc NODE_ENV=development webpack-dev-server --config webpack.config.js  --color --progress --hot",
    "dev": "npm run start",
    "build": "yarn install && npm run clean && npm run build-dll && cross-env-shell NODE_OPTIONS=--max_old_space_size=4096 ENV=prod NODE_ENV=production webpack -p --progress --colors --display-error-details --config webpack.config.js && node ./helps/start.js",
    "yunxi-dev": "yarn install && npm run clean && npm run build-dll && cross-env-shell NODE_OPTIONS=--max_old_space_size=4096 ENV=dev NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.config.js && node ./helps/start.js",
    "yunxi-test": "yarn install && npm run clean && npm run build-dll && cross-env-shell NODE_OPTIONS=--max_old_space_size=4096 ENV=test NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.config.js && node ./helps/start.js",
    "yunxi-prod": "yarn install && npm run clean && npm run build-dll && cross-env-shell NODE_OPTIONS=--max_old_space_size=4096 ENV=prod NODE_ENV=production  webpack --progress --colors --display-error-details --config webpack.config.js && node ./helps/start.js",
    "oss": "cross-env ENV=dev ./helps/start.js",
    "ossdev": "cross-env ENV=dev node ./helps/start.js",
    "osstest": "cross-env ENV=test node ./helps/start.js",
    "ossprod": "cross-env ENV=prod node ./helps/start.js",
    "clean": "rimraf dist/*"
  },
